{ 
  "parserOptions": { "ecmaVersion": 2020 },
  "plugins": ["jest"],
  "env": {
      "node": true,
      "es6": true
  },
  "rules": {
      "no-underscore-dangle": [2, { "allow": ["__filename", "__dirname"] }],
      "no-console": "off",
      "import/extensions": "off"
  },
  "extends": [
      "airbnb/base",
      "plugin:jest/recommended"
  ]
}